{
  "extends": "next/core-web-vitals",
  "plugins": [
    "unused-imports"
  ],
  "rules": {
    "prefer-const": "warn",
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "sort-imports": [
      "error",
      {
        "ignoreDeclarationSort": true
      }
    ]
  }
}